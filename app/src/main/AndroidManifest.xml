<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yang.myapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:windowSoftInputMode="adjustUnspecified|stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <!--编辑信息-->
        <activity android:name=".client.ui.activity.EditInfoActivity"/>
        <!--搜索Activity-->
        <activity android:name=".project.ui.activity.SearchActivity"/>
        <!--绑定手机-->
        <activity android:name=".my.ui.activity.BindPhoneActivity"/>
        <!--我的钱包-->
        <activity android:name=".my.ui.activity.MyWalletActivity"/>
        <!--修改密码-->
        <activity android:name=".my.ui.activity.ResetPswActivity"/>
        <!--修改密码 -->
        <activity android:name=".my.ui.activity.SettingActivity"/>



        <meta-data
            android:name="design_width"
            android:value="720" />

        <meta-data
            android:name="design_height"
            android:value="1280" />

    </application>
</manifest>