<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yang.myapplication">



    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:windowSoftInputMode="adjustUnspecified|stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <!--编辑信息-->
        <activity android:name=".client.ui.activity.EditInfoActivity"/>
        <!--搜索Activity-->
        <activity android:name=".project.ui.activity.SearchActivity"/>
        <!--绑定手机-->
        <activity android:name=".my.ui.activity.BindPhoneActivity"/>
        <!--我的钱包-->
        <activity android:name=".my.ui.activity.MyWalletActivity"/>
        <!--修改密码-->
        <activity android:name=".my.ui.activity.ResetPswActivity"/>
        <!--设置 -->
        <activity android:name=".my.ui.activity.SettingActivity"/>
        <!--关于速卖通 -->
        <activity android:name=".my.ui.activity.AboutActivity"/>
        <!--客户编辑 -->
        <activity android:name=".client.ui.activity.EditClientInfoActivity"/>
        <!--账号设置 -->
        <activity android:name=".my.ui.activity.AccountSettingActivity"/>
        <!--消息设置 -->
        <activity android:name=".my.ui.activity.MsgSettingActivity"/>
        <!--佣金详情 -->
        <activity android:name=".CommissionDetailActivity"/>
        <!--佣金结算 -->
        <activity android:name=".CommissionBalanceActivity"/>



        <!--AutoLayout标明设计图尺寸-->
        <!--app:layout_auto_basewidth="height"，代表height上编写的像素值参考宽度。-->
        <!--app:layout_auto_baseheight="width"，代表width上编写的像素值参考高度。-->
        <meta-data
            android:name="design_width"
            android:value="720" />

        <meta-data
            android:name="design_height"
            android:value="1280" />

    </application>
</manifest>